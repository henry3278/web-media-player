{
  "enabled": true,
  "targetWebsite": "https://www.kchai.org/",
  "imageSelectors": [
    "img[src*='.jpg']",
    "img[src*='.png']",
    "img[src*='.webp']"
  ],
  "excludeKeywords": [
    "icon",
    "logo",
    "ad",
    "spacer"
  ],
  "insertPosition": "after_first_sentence",
  "maxImageWidth": "400px",
  "requestTimeout": 5000,
  "cacheDuration": 300000,
  "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
}
